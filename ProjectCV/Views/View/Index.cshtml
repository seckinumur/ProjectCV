@model DAL.VM.VMUser
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Seçkin Umur Software Developer">
    <meta name="keywords" content="seçkin, umur, asp.net, csharp, mvc, .net, winform, xamarin, izmir, yazılım, web, programlama, seckinumur, seckinumur.com, izmir, bilgeadam, choice, corporation">
    <meta name="author" content="seçkin umur, seckinumur.com">
    <title>Seçkin Umur</title>
    <link rel="shortcut icon" type="image/png" href="~/img/favicon.png" />

    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <link href="~/Content/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/nivo-lightbox/nivo-lightbox.css" rel="stylesheet" />
    <link href="~/Content/nivo-lightbox/themes/default/default.css" rel="stylesheet" />
    <link href="~/Content/perfect-scrollbar/css/perfect-scrollbar.min.css" rel="stylesheet" />
    <link href="~/Content/owl-carousel/owl.carousel.css" rel="stylesheet" />
    <link href="~/Content/owl-carousel/owl.theme.css" rel="stylesheet" />
    <link href="~/Content/css/styles.css" rel="stylesheet" />

    <script src="~/Scripts/jquery-3.2.1.min.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/validate.js" type="text/javascript"></script>
    <script src="~/Scripts/holder.js" type="text/javascript"></script>
    <script src="~/Content/nivo-lightbox/nivo-lightbox.min.js" type="text/javascript"></script>
    <script src="~/Content/perfect-scrollbar/js/min/perfect-scrollbar.jquery.min.js" type="text/javascript"></script>
    <script src="~/Content/owl-carousel/owl.carousel.js" type="text/javascript"></script>
    <script src="~/Scripts/cross-browser.js" type="text/javascript"></script>
    <script src="~/Scripts/main.js" type="text/javascript"></script>

</head>
<body>

    <div class="loader-container" id="page-loader">
        <div class="loading-wrapper">
            <div class="loader-animation" id="loader-animation">
                <span class="loader"><span class="loader-inner"></span></span>
            </div>
            <div class="loader-name" id="loader-name">
                Seçkin <strong>UMUR</strong>
            </div>
            <p class="loader-job" id="loader-job">@Model.Meslek</p>
        </div>
    </div>

    <section id="body" class="">

        <div class="container">

            <div class="main-menu" id="main-menu">
                <ul class="main-menu-list">
                    <li><a href="#page-resume" class="link-home">Anasayfa</a></li>
                    <li><a href="#page-resume" class="link-page">Seçkin UMUR CV</a></li>
                    <li><a href="#page-CVHazirla" class="link-page">CV Hazırla!</a></li>
                    <li><a href="#page-contact" class="link-page">İletişim</a></li>
                    <li><a href="~/Admin/Login">Sisteme Giriş</a></li>
                </ul>
            </div>

            <div class="section-vcardbody section-home" id="section-home">
                <div class="vcard-profile-pic">
                    <img src="@Model.Resim" id="profile2" alt="" />
                    <img src="@Model.Resim" id="profile1" class="profileActive" alt="" />
                </div>
                <div class="vcard-profile-description">
                    <h1 class="profile-title"><span class="color1">@Model.AdSoyad</span></h1>
                    <h2 class="profile-subtitle">@Model.Meslek</h2>
                    <hr class="hr1">
                    <p>@Model.Beceriler</p>
                    <div class="vcard-profile-description-feature">
                        <div class="vcard-profile-description-ft-item">
                            <p><a class="text-danger" href="tel:@Model.Telefon">@Model.Telefon</a> - <a class="text-danger" href="mailto:@Model.email">@Model.email</a></p>
                            <p><a class="text-danger" href="@Model.website">@Model.website</a></p>
                        </div>
                    </div>
                </div>

            </div>

            <div class="section-vcardbody section-page" id="page-resume">

                <div class="section-education">
                    <h2 class="section-title">Seçkin UMUR CV</h2>
                    <div class="resume-buttons header-page-buttons">
                        <a href="~/CV/Index" class="btn btn-default btn-default2"><i class="fa fa-download"></i>&nbsp; PDF Olarak İndir</a>
                        <a href="#page-CVHazirla" class="btn btn-default btn-default2 link-page"><i class="fa fa-print"></i>&nbsp; CV Hazırla!</a>
                    </div>
                    <h2 class="section-title2"><i class="fa fa-language"></i>&nbsp; Konuşma Dilleri & Programlama Dilleri</h2>

                    <div class="resume-item">
                        <h4 class="section-item-title-1">@Model.Diller</h4>
                    </div>

                </div>

                <div class="section-education">
                    <h2 class="section-title2"><i class="fa fa-university"></i>&nbsp; Eğitim & Kurs</h2>
                    @{
                        foreach (var item in Model.Egitimler)
                        {
                            <div class="resume-item">
                                <h3 class="section-item-title-1">@item.Tanimi</h3>
                                <h4 class="graduation-time">@item.OkulAdi - <span class="graduation-date">@item.Tarih</span></h4>
                                <div class="graduation-description">
                                    <p>@item.YabanciDil</p>
                                </div>
                            </div>
                        }
                    }
                </div>

                <div class="section-education">
                    <h2 class="section-title2"><i class="fa fa-flag"></i>&nbsp; İş Deneyimi</h2>

                    @{
                        foreach (var item in Model.ProjelerDeneyimleri)
                        {
                            <div class="resume-item">
                                <h3 class="section-item-title-1">@item.ProjeAdi</h3>
                                <h4 class="job">@item.Teknoloji - <span class="job-date"><a href="@item.Link" target="_blank">@item.Link</a></span></h4>
                                <div class="graduation-description">
                                    <p>@item.Aciklama</p>
                                </div>
                            </div>
                        }
                    }
                </div>

                <div class="page-footer">
                    <p class="footer-quote">©2017 Choice Corporation All Rights Reserved.</p>
                </div>

            </div>

            <div class="section-vcardbody section-page" id="page-CVHazirla">
                <div class="section-blog">
                    <h2 class="section-title">CV HAzırlama Aracı</h2>
                    <div class="blog-item-title-wrapper"><h2 class="blog-item-title title-border">Nasıl Bir CV Hazırlayabilirim?</h2></div>
                    <div class="blog-posts">
                        
                        <div class="blog-item">
                            <div class="blog-item-wrapper">
                                <div class="blog-item-infos">
                                    <div class="blog-item-description text-justify">
                                        <p>Merhaba! Bu Uygulama Size ONE PAGE CV HAzırlamak İçin Araçlar Sunmaktadır. Sisteme Kayıt Olduktan Sonra Oturum Açtığınızda Kişisel Bilgilerinizi Girip Dinamik Olarak Oluşturabileceğiniz Kolay Bir Arayüz Sunmaktadır. Oluşturduğunuz CV Benzersiz Bir QR Kod İçermektedir. Bu kod qr okuyucularda taratılıp cv nin html5 versiyonuna ulaşılmasına olanak verir. CV Dinamik olarak hazırlanmaktır. Bu araç Ücretsizdir. <strong>Dikkat! Kişisel Bilgilerinizi Vermektesiniz. Bu Uygulama Kişisel Bilgilerinizi Korumakla Sorumlu Değildir. Ancak Hiçbir Zaman Kişisel Bilgilerinizi Reklam Amacıyla Kullanmayacaktır. CV Oluşturarak işbu Sözleşmeyi Kabul Etmiş Sayılırsınız.</strong></p>
                                    </div>
                                    <div class="blog-item-link">
                                        <a href="#" class="btn btn-default loadPost">Haydi Bir CV HAzırlayalım!!</a>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

            <div class="section-vcardbody section-page section-page-single" id="page-blog-single">
                <div class="section-blog-single">
                    <div id="postHere"></div>
                    <h4 class="contact-subtitle-1"><i class="fa fa-sign-in"></i>&nbsp; Sisteme Kayıt Ol!</h4>

                    <form  method="post" role="form" action="@Url.Action("Login","Admin")" >
                        <div class="form-group">
                            <input type="text" name="AdSoyad" tabindex="1" class="form-control" placeholder="Ad Soyad" required>
                        </div>
                        <div class="form-group">
                            <input type="email" name="email" tabindex="1" class="form-control" placeholder="E-Mail" required>
                        </div>
                        <div class="form-group">
                            <input type="password" name="Sifre" tabindex="2" class="form-control" placeholder="Şifre" required>
                        </div>
                        <div class="form-group">
                            <input type="submit" name="Komut" class="btn btn-default form-send" value="Kayit">
                        </div>
                    </form>

                    <div class="col-xs-12 form-group">
                        <img class="img-responsive center-block" width="150" src="~/img/choicecorporation.png" />
                    </div>
                    <div class="blog-item-infos">
                        <div class="blog-item-description text-justify">
                            <p class="text-danger">İşbu Anlaşma Choice Corp. ve son kullanıcı arasındadır. Kullanıcı sistemi kullanarak anlaşmayı kabul etmiş sayılır.
                                -Choice Corp Hiçbir Şekilde Kişisel Verilerinizi Reklam Amacıyla Kullanmayacağına Söz Verir.
                                -Choice Corp Olası Bir Ters Mühendislik HACK Girişimi Sonucunda Kişisel Bilgilerinizin Tutulduğu
                                Choice Corp Cloud Sunucularında Meydana Gelen Hasar ve veri kaybından Sorumlu Değildir.
                                -Choice Corp Güvenlik Sistemi Choice Security Services Kişisel Bilgilerinizin Güvenliğinden ve Parolalarınızın 
                                Güvenliğinden Sorumlu Değildir.
                                TÜM SORUMLULUK SİZE AİTTİR. SON KULLANICI SİSTEMİ KULLANARAK BU ANLAŞMAYI KABUL ETMİŞ SAYILIR.</p>
                        </div>
                    </div>
                    <div class="col-xs-12 form-group">
                        <a href="#page-CVHazirla" class="btn btn-default btn-block  link-page">Geri Dön!</a>
                    </div>
                </div>
            </div>

            <div class="section-vcardbody section-page" id="page-contact">
                <div class="section-contact">
                    <h2 class="section-title">İletişim</h2>
                    <div class="contact-infos">
                        <div class="col-lg-12 text-center">
                            <i class="fa fa-phone fa-3x sr-contact"></i>
                            <p>Aramak için sadece <a href="tel:+905423428009">Buraya Tıkla</a> Yada</p>
                            <p>+905423428009</p>
                        </div>
                        <hr class="hr1">
                        <div class="col-lg-12 text-center">
                            <i class="fa fa-envelope-o fa-3x sr-contact"></i>
                            <p><a href="mailto:seckinumur@gmail.com">seckinumur@gmail.com</a></p>
                        </div>
                        <hr class="hr1">
                        <div class="col-lg-12 text-center">
                            <img class="img-responsive center-block" width="100" src="~/img/qrseckin.png" />
                            <p>QR Kod İle Kişilere Hızlı Ekleme</p>
                        </div>
                        <hr class="hr1">
                        <div class="col-lg-12 text-center">
                            <div class="contact_social">
                                <a class="fb" a href="https://www.facebook.com/seckinumur85" target="_blank"><i class="fa fa-facebook fa-lg"></i></a>
                                <a class="twitter" a href="https://twitter.com/SeckinUmur" target="_blank"><i class="fa fa-twitter fa-lg"></i></a>
                                <a class="gplus" a href="https://github.com/seckinumur" target="_blank"><i class="fa fa-github fa-lg"></i></a>
                                <a class="pinterest" a href="https://tr.linkedin.com/in/seçkin-umur-710481104" target="_blank"><i class="fa fa-linkedin-square fa-lg"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>

    </section>

</body>
</html>