<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Choice Corporation Admin Kontrol sistemi">
    <meta name="keywords" content="seçkin,umur,seckinumur,Seckin,Umur,choice,Seçkin Umur">
    <meta name="author" content="Seçkin Umur">

    <link rel="icon" href="~/img/favicon.png" sizes="32x32">
    <title>@ViewBag.Title</title>

    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/metisMenu.min.css" rel="stylesheet" />
    <link href="~/Content/css/sb-admin-2.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/css/jasny-bootstrap.min.css" rel="stylesheet" />

    <script src="~/Scripts/jquery-3.2.1.min.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/metisMenu.min.js" type="text/javascript"></script>
    <script src="~/Scripts/sb-admin-2.js" type="text/javascript"></script>
    <script src="~/Scripts/jasny-bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap-notify.min.js" type="text/javascript"></script>
    

</head>
<body>
    <div id="wrapper">
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <img src="~/img/choicecorporation.png" class="pull-left navbar-brand" />
                <i class="navbar-brand"> <i style="font-style:normal" class="text-center">Personel Control Systems V.1.5 / <strong>@Session["Name"].ToString()</strong> Sisteme Oturum Açtı</i> </i>
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <ul class="nav navbar-top-links navbar-right">
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-tasks fa-fw"></i> <i class="fa fa-caret-down"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-tasks">
                        <li>
                            <a class="text-center" href="#">
                                <strong>Program Geliştirme Süreci</strong>
                            </a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="#" data-toggle="modal" data-target="#Hule">
                                <div>
                                    <p>
                                        <strong>Project CV V.1.5</strong>
                                        <span class="pull-right text-muted">80% Tamamlandı</span>
                                    </p>
                                    <div class="progress progress-striped active">
                                        <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100" style="width: 80%">
                                            <span class="sr-only">80% Tamamlandı</span>
                                        </div>
                                    </div>
                                </div>
                            </a>
                        </li>
                    </ul>
                </li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-user fa-fw"></i> <i class="fa fa-caret-down"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-user">
                        <li>
                            <a href="~/Admin/Logoff"><i class="fa fa-sign-out fa-fw"></i> Oturumu Kapat</a>
                        </li>
                    </ul>
                </li>
            </ul>
            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">
                        <li>
                            <a href="~/Admin/Admin"><i class="fa fa-user-circle"></i> Profil</a>
                        </li>
                        <li>
                            <a href="~/Admin/Proje"><i class="fa fa-briefcase"></i> Projeler</a>
                        </li>
                        <li>
                            <a href="~/Admin/Egitim"><i class="fa fa-university"></i> Eğitim Kurs</a>
                        </li>
                        <li>
                            <a href="~/Admin/IsDeneyimi"><i class="fa fa-building"></i>  İş Deneyimi</a>
                        </li>
                        <li>
                            <a href="~/CV/CV/@Session["User"]"><i class="fa fa-address-card-o"></i>  CV Görüntüle</a>
                        </li>
                        <li>
                            <a href="~/Admin/AdminKontrol"><i class="fa fa-eraser"></i>  Siteyi Sıfırla</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        @RenderBody()
    </div>
    <script type="text/javascript">

        $(document).ready(function () {
          @TempData["Veri"]
          @TempData["Uyari"]
        });

        function BildirimVarUyarisi() {
            $.notify({
                icon: 'fa fa-check-circle',
                message: 'Project CV V.1.5 Uygulamasına Hoşgeldiniz! Şimdi Tüm bilgilerinizi Eksiksiz Doldurup Dinamik CV Deneyimin Keyfine Varın',
            }, {
                    type: 'success',
                    placement: {
                        from: 'top',
                        align: 'center'
                    },
                    delay: 6000
                });
        };

        function Hosgeldin() {
            $.notify({
                icon: 'fa fa-check-circle',
                message: 'Tekrar Hoşgeldin!',
            }, {
                    type: 'success',
                    placement: {
                        from: 'top',
                        align: 'right'
                    },
                    delay: 3000
                });
        };

        function Basarili() {
            $.notify({
                icon: 'fa fa-check-circle',
                message: 'İşlem Başarılı',
            }, {
                    type: 'success',
                    placement: {
                        from: 'top',
                        align: 'center'
                    },
                    delay: 6000
                });
        };

        function Basarisiz() {
            $.notify({
                icon: 'fa fa-check-circle',
                message: 'İşlem Başarısız',
            }, {
                    type: 'danger',
                    placement: {
                        from: 'top',
                        align: 'center'
                    },
                    delay: 6000
                });
        };

    </script>
</body>
</html>